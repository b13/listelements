{
  "name": "b13/listelements",
  "type": "typo3-cms-extension",
  "description": "Adds list elements to tt_content",
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-backend": "^13.4 || ^14.1",
    "typo3/cms-fluid-styled-content": "^13.4 || ^14.1"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web",
      "app-dir": ".Build",
      "extension-key": "listelements"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "typo3/testing-framework": "^9.1",
    "typo3/cms-frontend": "^13.4 || ^14.1",
    "typo3/cms-workspaces": "^13.4 || ^14.1",
    "phpstan/phpstan": "^2.1",
    "typo3/coding-standards": "^0.8",
    "b13/listelements-example": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "B13\\Listelements\\Tests\\": "Tests"
    }
  },
  "autoload": {
    "psr-4": {
      "B13\\Listelements\\": "Classes/"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "Tests/Functional/Fixture/Extensions/*"
    }
  ]
}
